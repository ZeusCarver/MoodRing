<table class="table table-striped">
      <thead>
        <tr>
          <th>Profile ID #</th>
          <th>Name</th>
          <th>Current Mood Level</th>
        </tr>
      </thead>
      <tbody>
<%@moods.each do |mood|%>
        <tr>
         <td><%=mood.id%></td>
          <td><%=mood.name%></td>
          <td><%=mood.mood%></td>
        </tr>
      <%end%>
      </tbody>
    </table>

<br>

