<div class="container">
  <div class="row">
    <div class="col-md-10 col-md-offset-1">
<div id="carousel-example-captions" class="carousel slide" data-ride="carousel">
      <ol class="carousel-indicators">
        <li data-target="#carousel-example-captions" data-slide-to="0" class="active"></li>
        <li data-target="#carousel-example-captions" data-slide-to="1" class=""></li>
        <li data-target="#carousel-example-captions" data-slide-to="2" class=""></li>
      </ol>
      <div class="carousel-inner">
        <div class="item active">
          <img data-src="holder.js/900x500/auto/#777:#777" alt="900x500" src="/assets/Carousel1.jpg">
        </div>
        <div class="item">
          <img data-src="holder.js/900x500/auto/#666:#666" alt="900x500" src="/assets/Carousel2.jpg">
        </div>
        <div class="item">
          <img data-src="holder.js/900x500/auto/#555:#5555" alt="900x500" src="/assets/Carousel3.jpg">
        </div>
      </div>
      <a class="left carousel-control" href="#carousel-example-captions" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
      </a>
      <a class="right carousel-control" href="#carousel-example-captions" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
      </a>
    </div>
</div>
</div>
</div>
<!--   First row of profiles -->
  <div class="container"> 
   <div class="row">
<%@moods.each do |mood|%>
     <div class="col-md-4">
       <div class="thumbnail">
        <img src="https://encrypted-tbn2.gstatic.com/images?q=tbn:ANd9GcSKnEftL0s1WdMclajZXUoI3TJrvORV1xu0-cr6qMdbbDqGzOesl1nwIw" alt="News">
        <div class="caption">
          <p><strong><%=mood.name%></strong></p>
          <p><%=mood.mood%></p>
          <div class="progress">

<%if mood.mood == 0 or mood.mood == 1 or mood.mood == 2 or mood.mood == 3%>
   <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="<%=mood.mood%>0" aria-valuemin="0" aria-valuemax="100" style="width:<%=mood.mood%>0%">
<%elsif mood.mood == 4 or mood.mood == 5 or mood.mood == 6%>
    <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="<%=mood.mood%>0" aria-valuemin="0" aria-valuemax="100" style="width:<%=mood.mood%>0%">
<%else%>
<div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="<%=mood.mood%>0" aria-valuemin="0" aria-valuemax="100" style="width:<%=mood.mood%>0%">
<%end%>
    <span class="sr-only"><%=mood.mood%>0%</span>
  </div>
</div>
          <p><a href="/profile/<%=mood.id%>/show" class="btn btn-primary" role="button">Edit</a> </p>
        </div>
      </div>
     </div>
<%end%>
    </div>
</div>